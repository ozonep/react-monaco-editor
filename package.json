{
  "name": "react-monaco-hooks",
  "version": "0.26.1",
  "description": "Monaco Editor for React based on hooks + other optimizations",
  "main": "lib/index.js",
  "module": "lib/index",
  "jsnext:main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "preversion": "yarn lint",
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "format": "prettier-eslint --write src/**/*",
    "lint": "prettier-eslint --list-different src/**/*",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "test": "yarn test:typescript",
    "test:typescript": "typings-tester --dir test/typescript"
  },
  "keywords": [
    "monaco",
    "editor",
    "react",
    "vscode"
  ],
  "authors": [
    "Ivan Malkov <ozonep@yandex.ru>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ozonep/react-monaco-hooks/issues"
  },
  "homepage": "https://github.com/ozonep/react-monaco-hooks",
  "repository": "https://github.com/ozonep/react-monaco-hooks",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.8.6",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.2",
    "typings-tester": "^0.3.2"
  },
  "peerDependencies": {
    "react": "^16.x"
  },
  "dependencies": {
    "@types/react": "^16.8.13",
    "monaco-editor": "^0.16.2",
    "prop-types": "^15.7.2"
  }
}
